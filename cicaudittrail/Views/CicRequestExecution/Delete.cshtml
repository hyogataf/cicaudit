@model cicaudittrail.Models.CicRequestExecution

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>CicRequestExecution</legend>

    <div class="display-label">CicRequest</div>
    <div class="display-field">@(Model.CicRequest == null ? "None" : Model.CicRequest.Libelle)</div>

    <div class="display-label">CicRequestUserUpdated</div>
    <div class="display-field">@Model.CicRequestUserUpdated</div>

    <div class="display-label">CicRequestDateUpdated</div>
    <div class="display-field">@String.Format("{0:g}", Model.CicRequestDateUpdated)</div>

    <div class="display-label">CicRequestUserExecuted</div>
    <div class="display-field">@Model.CicRequestUserExecuted</div>

    <div class="display-label">CicRequestDateExecuted</div>
    <div class="display-field">@String.Format("{0:g}", Model.CicRequestDateExecuted)</div>

    <div class="display-label">CicRequestUserDeleted</div>
    <div class="display-field">@Model.CicRequestUserDeleted</div>

    <div class="display-label">CicRequestDateDeleted</div>
    <div class="display-field">@String.Format("{0:g}", Model.CicRequestDateDeleted)</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


