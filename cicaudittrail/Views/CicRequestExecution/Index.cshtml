@model IEnumerable<cicaudittrail.Models.CicRequestExecution>

@{
    ViewBag.Title = "Piste d'audit";
}
@Scripts.Render("~/bundles/datatablejs")
@Styles.Render("~/Content/datatablecss")
<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {
        $("#auditTable").dataTable({ bJQueryUI: false, sPaginationType: "full_numbers" });
    });
  
</script>

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Piste d'audit
        </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="panel panel-default">
  <!-- Default panel contents --> 
  <!-- Table -->
    <table class="table table-striped table-bordered table-hover" id="auditTable">
        <thead>
  <tr>
  <th></th>
          <th>  
            @Html.DisplayNameFor(model => model.CicRequest)
        </th>
        <th>  
            @Html.DisplayNameFor(model => model.CicRequestUserUpdated)
        </th>
        <th>  
            @Html.DisplayNameFor(model => model.CicRequestDateUpdated)
        </th>
        <th>  
            @Html.DisplayNameFor(model => model.CicRequestUserExecuted)
        </th>
        <th>  
            @Html.DisplayNameFor(model => model.CicRequestDateExecuted)
        </th> 
 
   </tr>
            </thead>
        <tbody>
   @foreach (var item in Model) {
   <tr>
           <td> 
            @Html.ActionLink("Details", "Details", new { id=item.CicRequestExecutionId })  
        </td>
        <td>
			@(item.CicRequest == null ? "None" : item.CicRequest.Libelle)
        </td>
        <td>
			@item.CicRequestUserUpdated
        </td>
        <td>
			@String.Format("{0:g}", item.CicRequestDateUpdated)
        </td>
        <td>
			@item.CicRequestUserExecuted
        </td>
        <td>
			@String.Format("{0:g}", item.CicRequestDateExecuted)
        </td> 

    </tr>
   }
            </tbody>
  </table>
</div>
 
