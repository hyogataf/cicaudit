@model cicaudittrail.Models.CicMessageMailDocuments

@{
    ViewBag.Title = "Consultation CicMessageMailDocuments";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Consultation CicMessageMailDocuments
            </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>  

@if (TempData["error"] != null) {
<div class="alert alert-danger" role="alert">
    @TempData["error"]
</div>
}

@if (TempData["message"] != null) {
<div class="alert alert-success" role="alert">
    @TempData["message"]
</div>
}

@if (TempData["warning"] != null) {
<div class="alert alert-warning" role="alert">
    @TempData["warning"]
</div>
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Basic Form Elements
            </div>
            <div class="panel-body">
                <div class="row" style="margin-left: -15px">
                    <div class="col-lg-12">
                        <fieldset disabled>
                             
                             
    
      <div class="form-group">
                            <label for="CicMessageMail">
                                CicMessageMail
                            </label>
                            <input class="form-control" id="CicMessageMail" type="text" placeholder="@Html.DisplayTextFor(_ => Model.CicMessageMail).ToString()"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="DocumentName">
                                DocumentName
                            </label>
                            <input class="form-control" id="DocumentName" type="text" placeholder="@Model.DocumentName"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="DocumentType">
                                DocumentType
                            </label>
                            <input class="form-control" id="DocumentType" type="text" placeholder="@Model.DocumentType"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="Document">
                                Document
                            </label>
                            <input class="form-control" id="Document" type="text" placeholder="@Html.DisplayTextFor(_ => Model.Document).ToString()"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="DateCreated">
                                DateCreated
                            </label>
                            <input class="form-control" id="DateCreated" type="text" placeholder="@String.Format("{0:g}", Model.DateCreated)"
            readonly>
                        </div>

                        </fieldset> 
                    <p>
<button type="button" class="btn btn-primary">
    @Html.ActionLink("Modifier", "Edit", new { id=Model.CicMessageMailDocumentsId })
     </button> 
     <button type="button" class="btn btn-default">
    @Html.ActionLink("Retour", "Index")
     </button> 
</p>
                      
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

 


