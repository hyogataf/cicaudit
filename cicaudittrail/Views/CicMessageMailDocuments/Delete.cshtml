@model cicaudittrail.Models.CicMessageMailDocuments

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>


@if (TempData["error"] != null) {
<div class="alert alert-danger" role="alert">
    @TempData["error"]
</div>
}

@if (TempData["message"] != null) {
<div class="alert alert-success" role="alert">
    @TempData["message"]
</div>
}

@if (TempData["warning"] != null) {
<div class="alert alert-warning" role="alert">
    @TempData["warning"]
</div>
}

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>CicMessageMailDocuments</legend>

    <div class="display-label">CicMessageMail</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.CicMessageMail).ToString()</div>

    <div class="display-label">DocumentName</div>
    <div class="display-field">@Model.DocumentName</div>

    <div class="display-label">DocumentType</div>
    <div class="display-field">@Model.DocumentType</div>

    <div class="display-label">Document</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.Document).ToString()</div>

    <div class="display-label">DateCreated</div>
    <div class="display-field">@String.Format("{0:g}", Model.DateCreated)</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


