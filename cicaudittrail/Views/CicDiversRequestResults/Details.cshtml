@model cicaudittrail.Models.CicDiversRequestResults

@{
    ViewBag.Title = "Consultation CicDiversRequestResults";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Consultation CicDiversRequestResults
            </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>  

@if (TempData["error"] != null) {
<div class="alert alert-danger" role="alert">
    @TempData["error"]
</div>
}

@if (TempData["message"] != null) {
<div class="alert alert-success" role="alert">
    @TempData["message"]
</div>
}

@if (TempData["warning"] != null) {
<div class="alert alert-warning" role="alert">
    @TempData["warning"]
</div>
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Basic Form Elements
            </div>
            <div class="panel-body">
                <div class="row" style="margin-left: -15px">
                    <div class="col-lg-12">
                        <fieldset disabled>
                             
                             
    
      <div class="form-group">
                            <label for="Code">
                                Code
                            </label>
                            <input class="form-control" id="Code" type="text" placeholder="@Model.Code"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="Criteria">
                                Criteria
                            </label>
                            <input class="form-control" id="Criteria" type="text" placeholder="@Model.Criteria"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="RowContent">
                                RowContent
                            </label>
                            <input class="form-control" id="RowContent" type="text" placeholder="@Model.RowContent"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="DateCreated">
                                DateCreated
                            </label>
                            <input class="form-control" id="DateCreated" type="text" placeholder="@String.Format("{0:g}", Model.DateCreated)"
            readonly>
                        </div>

                        </fieldset> 
                    <p>
<button type="button" class="btn btn-primary">
    @Html.ActionLink("Modifier", "Edit", new { id=Model.CicDiversRequestResultsId })
     </button> 
     <button type="button" class="btn btn-default">
    @Html.ActionLink("Retour", "Index")
     </button> 
</p>
                      
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

 


