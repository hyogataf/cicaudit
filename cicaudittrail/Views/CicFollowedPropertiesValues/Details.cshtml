@model cicaudittrail.Models.CicFollowedPropertiesValues

@{
    ViewBag.Title = "Consultation CicFollowedPropertiesValues";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Consultation CicFollowedPropertiesValues
            </h1>
    </div>
    <!-- /.col-lg-12 -->
</div>  

@if (TempData["error"] != null) {
<div class="alert alert-danger" role="alert">
    @TempData["error"]
</div>
}

@if (TempData["message"] != null) {
<div class="alert alert-success" role="alert">
    @TempData["message"]
</div>
}

@if (TempData["warning"] != null) {
<div class="alert alert-warning" role="alert">
    @TempData["warning"]
</div>
}

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                Basic Form Elements
            </div>
            <div class="panel-body">
                <div class="row" style="margin-left: -15px">
                    <div class="col-lg-12">
                        <fieldset disabled>
                             
                             
    
      <div class="form-group">
                            <label for="CicRequestResultsFollowed">
                                CicRequestResultsFollowed
                            </label>
                            <input class="form-control" id="CicRequestResultsFollowed" type="text" placeholder="@Html.DisplayTextFor(_ => Model.CicRequestResultsFollowed).ToString()"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="Property">
                                Property
                            </label>
                            <input class="form-control" id="Property" type="text" placeholder="@Model.Property"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="Value">
                                Value
                            </label>
                            <input class="form-control" id="Value" type="text" placeholder="@Model.Value"
            readonly>
                        </div>

    
      <div class="form-group">
                            <label for="DateCreated">
                                DateCreated
                            </label>
                            <input class="form-control" id="DateCreated" type="text" placeholder="@String.Format("{0:g}", Model.DateCreated)"
            readonly>
                        </div>

                        </fieldset> 
                    <p>
<button type="button" class="btn btn-primary">
    @Html.ActionLink("Modifier", "Edit", new { id=Model.CicFollowedPropertiesValuesId })
     </button> 
     <button type="button" class="btn btn-default">
    @Html.ActionLink("Retour", "Index")
     </button> 
</p>
                      
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

 


